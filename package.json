{
  "name": "react-streaming-sandbox",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "files": [
    "dist",
    "wrangler.toml"
  ],
  "scripts": {
    "dev": "wrangler pages dev",
    "start": "wrangler pages dev",
    "build": "node build.js",
    "test": "vitest",
    "prepare": "husky",
    "tsc": "tsc -p jsconfig.json",
    "format": "prettier --no-error-on-unmatched-pattern --write **/*.{js,jsx,ts,tsx,css,md,html,yml,yaml,json}",
    "format:check": "prettier --no-error-on-unmatched-pattern --check **/*.{js,jsx,ts,tsx,css,md,html,yml,yaml,json}",
    "lint-staged": "lint-staged",
    "lint": "npm run tsc && npm run format:check",
    "lint:fix": "npm run format"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.1.0",
    "@cloudflare/workers-types": "^4.20240402.0",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "esbuild": "^0.20.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "vitest": "1.3.0",
    "wrangler": "^3.47.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,css,md,html,yml,yaml,json}": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": "20.12.1"
  }
}
