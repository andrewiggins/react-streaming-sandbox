{
	"compilerOptions": {
		"checkJs": true,
		"noEmit": true,
		"target": "ES2021",
		"module": "ES2022",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"types": ["@cloudflare/workers-types"]
	}
}
