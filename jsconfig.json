{
	"compilerOptions": {
		"checkJs": true,
		"noEmit": true,
		"lib": ["es2021"],
		"target": "ES2021",
		"module": "NodeNext",
		"moduleResolution": "nodenext",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"strict": true,
		"jsx": "react-jsx",
		"types": ["@cloudflare/workers-types", "react/canary", "react-dom/canary"]
	},
	"include": ["src", "_worker.js"],
	"exclude": ["node_modules"]
}
